{
  "manifest_version": 2,
  "name": "Volume Manager",
  "version": "1.0",
  "description": "Control any tab's volume separately",
  "permissions": ["activeTab", "tabCapture", "tabs"],
  "browser_action": {
    "default_icon": "img3.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "512": "img3.png"
  }
}
